{
	"name": "boilerplate-express-api",
	"version": "0.3.0",
	"description": "Boilerplate Express API",
	"engines": {
		"node": "10.x"
	},
	"scripts": {
		"postinstall": "rm -rf dist && mkdir dist && npx babel src --out-dir dist",
		"start": "cd dist && node server.js",
		"dev": "npm run postinstall && npm start",
		"watch": "babel-watch src/server.js",
		"build": "./build.sh"
	},
	"keywords": [
		"api",
		"boilerplate",
		"express",
		"es6"
	],
	"dependencies": {
		"bcrypt": "^3.0.7",
		"body-parser": "^1.18.3",
		"chalk": "^2.4.1",
		"compression": "^1.7.3",
		"cors": "^2.8.4",
		"ejs": "^2.7.4",
		"express": "^4.16.3",
		"fiware-iota": "0.0.1",
		"http-status-codes": "^1.4.0",
		"mosca": "^2.8.3"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-preset-env": "^1.7.0",
		"babel-watch": "^2.0.7"
	}
}
